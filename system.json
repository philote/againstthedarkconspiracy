{
  "id": "againstthedarkconspiracy",
  "title": "Against the Dark Conspiracy",
  "description": "The Against the Dark Conspiracy system for FoundryVTT!",
  "version": "#{VERSION}#",
  "compatibility": {
    "minimum": 12,
    "verified": 12.331,
    "maximum": 12
  },
  "authors": [
    {
      "name": "ephson",
      "url": "https://github.com/philote",
      "discord": "ephson#3451",
      "reddit": "u/ephson"
    }
  ],
  "esmodules": ["module/atdc.mjs"],
  "styles": ["css/atdc.css"],
  "packs": [
    {
      "name": "gear",
      "label": "Gear",
      "system": "againstthedarkconspiracy",
      "path": "./packs/gear.db",
      "type": "Item",
      "ownership": {
        "PLAYER": "OBSERVER",
        "TRUSTED": "OBSERVER",
        "ASSISTANT": "OWNER",
        "GAMEMASTER": "OWNER"
      }
    },
    {
      "name": "powers",
      "label": "NPC Powers",
      "system": "againstthedarkconspiracy",
      "path": "./packs/npc-powers.db",
      "type": "Item"
    },
    {
      "name": "compulsion",
      "label": "NPC Compulsions",
      "system": "againstthedarkconspiracy",
      "path": "./packs/npc-compulsions.db",
      "type": "Item"
    },
    {
      "name": "deterrent",
      "label": "NPC Deterrents",
      "system": "againstthedarkconspiracy",
      "path": "./packs/npc-deterrents.db",
      "type": "Item"
    },
    {
      "name": "vulnerability",
      "label": "NPC Vulnerabilities",
      "system": "againstthedarkconspiracy",
      "path": "./packs/npc-vulnerabilities.db",
      "type": "Item"
    },
    {
      "name": "complications-details",
      "label": "Complication Details Tables",
      "system": "againstthedarkconspiracy",
      "path": "./packs/complication-details.db",
      "type": "RollTable"
    },
    {
      "name": "possible-complications",
      "label": "Possible Complications Table",
      "system": "againstthedarkconspiracy",
      "path": "./packs/possible-complications-table.db",
      "type": "RollTable"
    },
    {
      "name": "random-lead",
      "label": "Random Lead Table",
      "system": "againstthedarkconspiracy",
      "path": "./packs/random-lead.db",
      "type": "RollTable"
    },
    {
      "name": "operational-details",
      "label": "Operational Details Tables",
      "system": "againstthedarkconspiracy",
      "path": "./packs/operational-details.db",
      "type": "RollTable"
    },
    {
      "name": "operational-questions",
      "label": "Operational Questions Tables",
      "system": "againstthedarkconspiracy",
      "path": "./packs/operational-questions.db",
      "type": "RollTable"
    }
  ],
  "packFolders": [
    {
      "name": "NPC Powers, Weaknesses & Vulnerabilities",
      "sorting": "m",
      "color": "#380000",
      "packs": ["powers", "vulnerability"],
      "folders": [
        {
          "name": "NPC Weaknesses",
          "sorting": "m",
          "color": "#380000",
          "packs": ["compulsion", "deterrent"]
        }
      ]
    },
    {
      "name": "Briefing Tables",
      "sorting": "m",
      "color": "#001d38",
      "packs": ["random-lead", "operational-questions", "operational-details"]
    },
    {
      "name": "Complication Tables",
      "sorting": "m",
      "packs": ["possible-complications", "complications-details"]
    }
  ],
  "languages": [
    {
      "lang": "en",
      "name": "English",
      "path": "lang/en.json"
    }
  ],
  "primaryTokenAttribute": "stress",
  "secondaryTokenAttribute": "intel",
  "url": "#{URL}#",
  "manifest": "#{MANIFEST}#",
  "download": "#{DOWNLOAD}#",
  "license": "LICENSE",
  "readme": "README.md",
  "bugs": "https://github.com/philote/againstthedarkconspiracy/issues",
  "media": [
    {
      "type": "screenshot",
      "url": "https://raw.githubusercontent.com/philote/againstthedarkconspiracy/main/assets/screenshot.webp",
      "loop": false,
      "caption": "Against the Dark Conspiracy screenshot from Foundry VTT."
    },
    {
      "type": "cover",
      "url": "https://raw.githubusercontent.com/philote/againstthedarkconspiracy/main/assets/cover.webp",
      "loop": false,
      "caption": "Cover Images for the Unofficial Against the Dark Conspiracy package."
    },
    {
      "type": "setup",
      "url": "https://raw.githubusercontent.com/philote/againstthedarkconspiracy/main/assets/package_image.webp",
      "thumbnail": "https://raw.githubusercontent.com/philote/againstthedarkconspiracy/main/assets/package_image_thumb.webp"
    }
  ]
}
